{
  "name": "avatar-builder",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "RPG Avatar Builder",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Arsen A. Gutsal <a.gutsal@softsky.company>",
  "private": true,
  "scripts": {
    "dev": "npx quasar dev",
    "build": "npx quasar build",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "test:e2e": "npx wdio ./test/webdriver/config/wdio.conf.js",
    "test:e2e:appium": "npx wdio ./test/webdriver/config/wdio.appium.conf.js",
    "test:e2e:electron": "npx wdio ./test/webdriver/config/wdio.electron.conf.js",
    "selenium:install": "npx selenium-standalone install",
    "selenium:start": "npx selenium-standalone start",
    "electron-chromedriver": "./node_modules/electron-chromedriver/bin/chromedriver --port=9515 --url-base=wd/hub --verbose"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.1",
    "lodash": "^4.17.15",
    "quasar": "^1.0.0",
    "vue-i18n": "^8.0.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "@quasar/cli": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-webdriver": "^1.0.0-beta.9",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@wdio/cli": "^5.13.1",
    "@wdio/local-runner": "^5.13.1",
    "babel-eslint": "^10.0.1",
    "electron-chromedriver": "^6.0.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "sinon": "^7.4.2",
    "vue-jest": "^3.0.5"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
